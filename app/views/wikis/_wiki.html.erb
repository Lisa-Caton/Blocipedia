<div class="media">
  <div class="media-body">
  <h4 class="media-heading" style="display:inline-block">
    <%= wiki.title %></h4> 
    | <small><%= link_to 'Show', wiki_path(wiki.id) %></small>
    | <small><%= link_to 'Edit', edit_wiki_path(wiki.id) %></small>

    | <small><%= link_to 'Destroy', wiki_path(wiki.id),
      method: :delete,
      data: { confirm: 'Are you sure you want to delete this wiki?' }
     %></small>

  </div>
</div>